import java.lang.*;
import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int num = 0, count = 0;
        for(int i = 0; i < N; i++) {
            num = sc.nextInt();
            count = 0;
            do {
                count++;
                num /= 10;
            } while(num > 0);
            System.out.println(count);
        }
    }
}
