import java.lang.*;
import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int T = sc.nextInt();
        while(T-- > 0) {
            int N = sc.nextInt();
            int last = N % 10;
            while(N % 10 != N) {
                N /= 10;
            }
            System.out.println(N + " " + last);
        }
    }
}
