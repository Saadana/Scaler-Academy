public class Solution {
    public ArrayList<Integer> solve(ArrayList<Integer> A) {
        ArrayList<Integer> res = new ArrayList<>();
        int n = A.size();
        if(n > 1) {
            for(int i = 0; i < n-1; ++i) {
                res.add(A.get(i+1) - A.get(i));
            }
        }
        return res;
    }
}
