public class Solution {
    // DO NOT MODIFY THE LIST. IT IS READ ONLY
    public ArrayList<Integer> solve(final List<Integer> A) {
        ArrayList<Integer> res = new ArrayList<>();
        int n = A.size();
        for(int i = n-1; i >= 0; --i) {
            res.add(A.get(i));
        }
        return res;
    }
}
